package com.practice;

public class ArraySum {

	public static void main(String[] args) {

		int a1[] = { 2, 4, 5, 6, 7, 8 };
		int sum1 = 0;
		int sum=0;
		for (int i : a1)
			sum1 += i;
		int a2[] = { 2, 4, 5, 6, 7, 8 };
		int sum2 = 0;

		for (int i : a1)
			sum2 += i;
		
		sum=sum1+sum2;
		System.out.println(sum);
	}

}
