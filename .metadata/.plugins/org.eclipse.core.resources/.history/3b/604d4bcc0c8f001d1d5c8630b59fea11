package com.practice;

public class CountOcurance {

	public static void main(String[] args) {

		String str = "aaabbbccc";

		int count = 0;
		char c;
		String serach = "aaabbbccc";

		for (int i = 0; i < str.length(); i++) {

			c = str.charAt(i);
			String string = Character.toString(c);
			if (string == serach) {

				count++;
			}

		}

	}

}
